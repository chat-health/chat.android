	        <View
	            android:id="@+id/health_topic5_divider"
	            style="@style/titleDivider"
	            android:background="@color/attendance"
	            android:layout_below="@id/health_topic5_title_field" />
	        <ImageView
		        android:id="@+id/health_topic5_button_img"
		        style="@style/themeImage"
		        android:layout_below="@id/health_topic5_title_field"
		        android:layout_marginTop="20dp"
		        android:src="@drawable/children_play_screenshot"
		        android:onClick="openHealthDelivery" />
	        <ImageView
                android:id="@+id/health_topic5_checkmark"
                style="@style/checkmarkImage"
                android:layout_below="@id/health_topic5_title_field"
                android:src="@drawable/checkmark"
                android:visibility="invisible" />
	       	<ImageButton
	       	    android:id="@+id/health_topic5_button"
	       	    style="@style/themeImageButton"
	       	    android:layout_alignTop="@+id/health_topic5_button_img"
	       	    android:layout_toRightOf="@+id/health_topic5_button_img"
	       	    android:layout_marginLeft="20dp"
	       	    android:layout_marginTop="40dp"
	       	    android:src="@drawable/children_play_btn_zulu"
	       	    android:onClick="openHealthDelivery" />   
		</RelativeLayout>
		<RelativeLayout
   	        android:layout_width="match_parent"
       		android:layout_height="wrap_content"
       		android:layout_weight="1"
       		android:layout_marginTop="5dp"
       		android:layout_marginLeft="10dp"
       		android:layout_marginRight="10dp">
	       		       	
      	    <TextView
	            android:id="@+id/health_topic6_title_field"
	            style="@style/titleTextLarge"
	            android:textColor="@color/attendance"
	            android:text="@string/health_topic_title_text" />
	        <View
	            android:id="@+id/health_topic6_divider"
	            style="@style/titleDivider"
	            android:background="@color/attendance"
	            android:layout_below="@id/health_topic6_title_field" />
	        <ImageView
		        android:id="@+id/health_topic6_button_img"
		        style="@style/themeImage"
		        android:layout_below="@id/health_topic6_title_field"
		        android:layout_marginTop="20dp"
		        android:src="@drawable/children_play_screenshot"
		        android:onClick="openHealthDelivery" />
	        <ImageView
                android:id="@+id/health_topic6_checkmark"
